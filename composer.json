{
    "name": "woodynew/hyperf-thrift-client",
    "description": "thrift client framework on hyperf server",
    "keywords": [
        "php",
        "swoole",
        "hyperf",
        "thrift",
        "rpc"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "woody",
            "email": "78925880@qq.com"
        }
    ],
    "require": {
        "php": ">=8.0",
        "hyperf/utils": "~3.0.0",
        "apache/thrift": "^0.18.0"
    },
    "autoload": {
        "psr-4": {
            "Woodynew\\Hyperf\\ThriftClient\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "tests\\": "tests/"
        }
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "hyperf": {
            "config": "Woodynew\\Hyperf\\ThriftClient\\ConfigProvider"
        }
    }
}